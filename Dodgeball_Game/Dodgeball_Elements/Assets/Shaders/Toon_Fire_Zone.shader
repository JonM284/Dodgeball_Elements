			CBUFFER_START(UnityPerMaterial)
			float4 _Explosion_Color;
			float4 _Second_Color;
			float _Speed_Mod_Horizontal;
			float _Speed_Mod_Vertical;
			float _Brightness_First;
			float _Second_Vertical_Speed;
			float _Second_Size;
